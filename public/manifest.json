{
	"name": "ThirdStats",
	"version": "1.7.0",
	"manifest_version": 2,
	"description": "__MSG_extensionDescription__",
	"author": "Andreas MÃ¼ller",
	"homepage_url": "https://github.com/devmount/third-stats",
	"default_locale": "en",
	"applications": {
		"gecko": {
			"id": "thirdstats@devmount.de",
			"strict_min_version": "78.0"
		}
	},
	"browser_action": {
		"default_popup": "popup.html",
		"default_title": "ThirdStats",
		"default_icon": "./icon.svg"
	},
	"background": {
		"scripts": [
			"./js/background.js"
		]
	},
	"options_ui": {
		"page": "./options.html",
		"open_in_tab": false,
		"browser_style": true
	},
	"permissions": [
		"accountsRead",
		"messagesRead",
		"storage",
		"downloads"
	],
	"icons": {
		"64": "./icon.svg",
		"32": "./icon.svg",
		"16": "./icon.svg"
	}
}
