{
	"name": "ThirdStats",
	"version": "0.6.0",
	"manifest_version": 2,
	"description": "__MSG_extensionDescription__",
	"author": "Andreas MÃ¼ller",
	"homepage_url": "https://github.com/devmount/third-stats",
	"default_locale": "en",
	"applications": {
			"gecko": {
					"id": "thirdstats@devmount.de",
					"strict_min_version": "78.0"
			}
	},
	"browser_action": {
			"default_popup": "popup.html",
			"default_title": "ThirdStats",
			"default_icon": "icon.svg"
	},
	"options_ui": {
		"page": "options.html"
	},
	"permissions": [
		"accountsRead",
		"messagesRead",
		"tabs",
		"storage"
	],
	"icons": {
			"64": "icon.svg",
			"32": "icon.svg",
			"16": "icon.svg"
	},
	"experiment_apis": {
		"LegacyPrefs": {
			"schema": "api/LegacyPrefs/schema.json",
			"parent": {
				"scopes": ["addon_parent"],
				"paths": [["LegacyPrefs"]],
				"script": "api/LegacyPrefs/implementation.js"
			}
		}
	}
}
